<?xml version='1.0' encoding='utf-8'?>
<dashboard>
	<label>Summary: Info</label>

	<row>
		<chart>
			<title>Info Components</title>
			<searchName>RBSH - Info Components</searchName>
			<option name="charting.axisTitleX.text">Component</option>
      <option name="charting.axisTitleY.text">Events</option>
      <option name="charting.axisY.scale">log</option>
      <option name="charting.chart">bar</option>
      <option name="charting.chart.axisX">Component</option>
      <option name="charting.chart.axisY">Events</option>
      <option name="charting.primaryAxisTitle.text"/>
      <option name="charting.secondaryAxis.scale">log</option>
      <option name="charting.secondaryAxisTitle.text"></option>
		</chart>
		<chart>
			<title>Info Hosts</title>
			<searchName>RBSH - Info Hosts</searchName>
			<option name="charting.chart">pie</option>
		</chart>
		<chart>
			<title>Top Optimizations</title>
			<earliestTime>-4h</earliestTime>
			<searchName>RBSH - Info Apps</searchName>
			<option name="charting.chart">pie</option>
		</chart>
	</row>
	<row>
		<chart>
			<searchName>RBSH - Info Timechart</searchName>
			<title>Info Over Time</title>
			<earliestTime>-1d</earliestTime>
	    <option name="charting.axisTitleX.text">Time</option>
      <option name="charting.axisTitleY.text">Events</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">column</option>
      <option name="charting.chart.stackMode">stacked</option>
      <option name="charting.primaryAxisTitle.text"/>
      <option name="charting.secondaryAxis.scale">linear</option>
      <option name="charting.secondaryAxisTitle.text"/>
    </chart>
	</row>

</dashboard>
